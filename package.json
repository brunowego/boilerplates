{
  "name": "@brunowego/nestjs-base",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start -w",
    "dev:debug": "nest start -wd",
    "build": "pnpm db:generate && nest build",
    "start": "nest start",
    "start:prod": "node ./dist/main.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:reset": "ts-node ./db/tasks/reset.ts",
    "db:seed": "pnpm db:generate && pnpm db:push && pnpm db:reset && ts-node ./db/tasks/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ./node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "TEST_ENV=e2e jest"
  },
  "dependencies": {
    "@nestjs/axios": "~2.0.0",
    "@nestjs/common": "~9.3.8",
    "@nestjs/config": "~2.3.1",
    "@nestjs/core": "~9.3.8",
    "@nestjs/platform-express": "~9.3.8",
    "@nestjs/terminus": "~9.2.0",
    "@prisma/client": "~4.10.1",
    "class-validator": "~0.14.0",
    "express-graphql": "~0.12.0",
    "graphql": "~15.3.0",
    "graphql-fields": "~2.0.3",
    "graphql-scalars": "~1.20.1",
    "graphql-yoga": "~3.7.0",
    "joi": "~17.7.1",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~7.8.0",
    "tslib": "~2.5.0",
    "type-graphql": "~1.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "~7.6.0",
    "@jest/types": "~29.4.2",
    "@nestjs/cli": "~9.2.0",
    "@nestjs/schematics": "~9.0.4",
    "@nestjs/testing": "~9.3.8",
    "@types/express": "~4.17.17",
    "@types/graphql-fields": "~1.3.4",
    "@types/jest": "~29.2.4",
    "@types/node": "~18.13.0",
    "@types/supertest": "~2.0.12",
    "@typescript-eslint/eslint-plugin": "~5.51.0",
    "@typescript-eslint/parser": "~5.51.0",
    "axios": "~1.3.2",
    "eslint": "~8.34.0",
    "eslint-config-prettier": "~8.6.0",
    "eslint-plugin-prettier": "~4.2.1",
    "fishery": "~2.2.2",
    "jest": "~29.3.1",
    "prettier": "~2.8.4",
    "prettier-plugin-prisma": "~4.10.0",
    "prisma": "~4.10.1",
    "prisma-erd-generator": "~1.2.5",
    "source-map-support": "~0.5.21",
    "supertest": "~6.3.3",
    "ts-jest": "~29.0.3",
    "ts-loader": "~9.4.2",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.1.1",
    "typegraphql-prisma": "~0.24.0",
    "typescript": "~4.9.5",
    "webpack": "~5.75.0"
  },
  "engines": {
    "node": ">=18 <19",
    "npm": ">=8 <9"
  },
  "prisma": {
    "schema": "./db/prisma/schema.prisma"
  }
}
