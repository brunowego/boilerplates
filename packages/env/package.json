{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@acme/env",
  "version": "0.0.0",
  "private": true,
  "license": "COPYRIGHT",
  "typesVersions": {
    "*": {
      "web": [
        "./dist/web.d.ts"
      ]
    }
  },
  "exports": {
    "./web": {
      "types": "./dist/web.d.ts",
      "import": "./dist/web.js",
      "require": "./dist/web.js",
      "default": "./dist/web.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./{.turbo,dist}",
    "lint": "eslint --ext .ts ./",
    "build": "tsc -p ./tsconfig.build.json"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "~0.7.1",
    "zod": "~3.22.4"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:~",
    "@types/node": "~18.18.14",
    "eslint-config-acme": "workspace:~",
    "rimraf": "~5.0.5",
    "typescript": "~5.3.2"
  }
}
