    "@radix-ui/react-slot": "~1.0.2",
    "class-variance-authority": "~0.7.0",
    "clsx": "~2.1.1",
    "lucide-react": "~0.390.0",
    "next-themes": "~0.3.0",
    "react-hook-form": "~7.51.5",
    "sonner": "~1.4.41",
    "tailwind-merge": "~2.3.0",
    "tailwindcss": "~3.4.4",
    "tailwindcss-animate": "~1.0.7",
    "zod": "~3.23.8"
  },
  "devDependencies": {
    "@acme/typescript-config": "workspace:~",
    "@types/node": "~20.14.2",
    "@types/react": "~18.3.3",
    "@types/react-dom": "~18.3.0",
    "react": "~18.3.1",
    "typescript": "~5.4.5"
  }
}
