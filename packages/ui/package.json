{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@acme/ui",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components/*": "./src/components/*.tsx",
    "./hooks/*": "./src/hooks/*.ts",
    "./lib/*": "./src/lib/*.ts",
    "./providers/*": "./src/providers/*.ts",
    "./styles.css": "./src/styles/index.css",
    "./tailwind.config": "./tailwind.config.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "scripts": {
