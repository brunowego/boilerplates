{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@acme/backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./{.cache,.turbo,build,dist}",
    "db:migrate": "medusa migrations run",
    "db:seed": "medusa seed -f ./data/seed.json",
    "dev": "medusa develop",
    "build": "npm run clean && npm run build:server && npm run build:admin",
    "build:server": "tsc -p ./tsconfig.server.json",
    "build:admin": "medusa-admin build"
  },
  "dependencies": {
    "@medusajs/admin": "~7.1.8",
    "@medusajs/cache-redis": "~1.9.0",
    "@medusajs/event-bus-redis": "~1.8.10",
    "@medusajs/file-local": "~1.0.2",
    "@medusajs/medusa": "~1.18.1",
    "medusa-fulfillment-manual": "~1.1.38",
    "medusa-payment-manual": "~1.0.24"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "rimraf": "~5.0.5"
  }
}
