{
  "name": "@acme/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start -w --preserveWatchOutput",
    "test": "jest",
    "test:watch": "jest --color --watch",
    "test:cov": "jest --color --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ./node_modules/.bin/jest -i",
    "test:e2e": "jest -c ./test/jest-e2e.config.ts",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start -wd",
    "start:prod": "node ./dist/main"
  },
  "dependencies": {
    "@nestjs/axios": "~3.0.3",
    "@nestjs/common": "~10.4.1",
    "@nestjs/config": "~3.2.3",
    "@nestjs/core": "~10.4.1",
    "@nestjs/mongoose": "~10.0.10",
    "@nestjs/platform-express": "~10.4.1",
    "@nestjs/terminus": "~10.2.3",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.1",
    "joi": "~17.13.3",
    "mongoose": "~8.5.4",
    "reflect-metadata": "~0.2.2",
    "rxjs": "~7.8.1"
  },
  "devDependencies": {
    "@acme/typescript-config": "workspace:~",
    "@nestjs/cli": "~10.4.4",
    "@nestjs/schematics": "~10.1.4",
    "@nestjs/testing": "~10.4.1",
    "@types/express": "~4.17.21",
    "@types/jest": "~29.5.12",
    "@types/node": "~22.5.0",
    "@types/supertest": "~6.0.2",
    "jest": "~29.7.0",
    "source-map-support": "~0.5.21",
    "supertest": "~7.0.0",
    "ts-jest": "~29.2.5",
    "ts-loader": "~9.5.1",
    "ts-node": "~10.9.2",
    "tsconfig-paths": "~4.2.0",
    "typescript": "~5.5.4"
  }
}
