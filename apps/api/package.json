{
  "name": "@acme/api",
  "version": "0.0.0",
  "private": true,
  "license": "COPYRIGHT",
  "scripts": {
    "clean": "rimraf ./{.turbo,dist}",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "dotenv -- tsx ./src/db/migrate.ts",
    "db:seed": "dotenv -- tsx ./src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "nest start -w --preserveWatchOutput",
    "lint": "eslint --ext .ts ./{src,test}",
    "test": "jest",
    "test:watch": "jest --color --watch",
    "test:cov": "jest --color --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ./node_modules/.bin/jest -i",
    "test:e2e": "jest -c ./test/jest-e2e.json",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start -wd",
    "start:prod": "node ./dist/main"
  },
  "dependencies": {
    "@nestjs/axios": "~3.0.1",
    "@nestjs/common": "~10.3.0",
    "@nestjs/config": "~3.1.1",
    "@nestjs/core": "~10.3.0",
    "@nestjs/platform-express": "~10.3.0",
    "@nestjs/terminus": "~10.2.0",
    "@paralleldrive/cuid2": "~2.2.2",
    "drizzle-orm": "~0.29.3",
    "postgres": "~3.4.3",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~7.8.1",
    "zod": "~3.22.4"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:~",
    "@acme/typescript-config": "workspace:~",
    "@faker-js/faker": "~8.3.1",
    "@nestjs/cli": "~10.3.0",
    "@nestjs/schematics": "~10.1.0",
    "@nestjs/testing": "~10.3.0",
    "@types/express": "~4.17.21",
    "@types/jest": "~29.5.11",
    "@types/node": "~20.11.5",
    "@types/supertest": "~6.0.2",
    "dotenv": "~16.3.1",
    "drizzle-kit": "~0.20.13",
    "eslint": "~8.56.0",
    "jest": "~29.7.0",
    "prettier": "~3.2.4",
    "rimraf": "~5.0.5",
    "source-map-support": "~0.5.21",
    "supertest": "~6.3.4",
    "ts-jest": "~29.1.1",
    "ts-loader": "~9.5.1",
    "ts-node": "~10.9.2",
    "tsconfig-paths": "~4.2.0",
    "tsx": "~4.7.0",
    "typescript": "~5.3.3"
  }
}
