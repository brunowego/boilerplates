{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@acme/medusa-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./{.cache,.turbo,build,dist}",
    "db:migrate": "medusa migrations run",
    "db:seed": "medusa seed -f ./data/seed.json",
    "dev": "medusa develop",
    "build": "bun build:server && bun build:admin",
    "build:server": "tsc -p ./tsconfig.server.json",
    "build:admin": "medusa-admin build",
    "preview": "medusa start",
    "start": "node --preserve-symlinks --trace-warnings ./index.js"
  },
  "dependencies": {
    "@medusajs/admin": "~7.1.10",
    "@medusajs/cache-redis": "~1.9.0",
    "@medusajs/event-bus-redis": "~1.8.10",
    "@medusajs/file-local": "~1.0.2",
    "@medusajs/medusa": "~1.20.0",
    "express": "~4.18.2",
    "medusa-fulfillment-manual": "~1.1.39",
    "medusa-payment-manual": "~1.0.24"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:~",
    "@acme/typescript-config": "workspace:~",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/express": "~4.17.21",
    "babel-cli": "~6.26.0",
    "prettier": "~3.2.4",
    "prettier-plugin-tailwindcss": "~0.5.11",
    "rimraf": "~5.0.5"
  }
}
